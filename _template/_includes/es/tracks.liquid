<stellar-layout class="bg-theme-base0 dm-bg-theme-base9" id="tracks" size="full">
    <stellar-layout padding="none" size="small">
        <copy-wrap align="center">
            <h2 class="fw7 mb0">{{es.event.tracks.title}}</h2>
            <p class="theme-base5 dm-theme-base2 mt0">{{es.event.tracks.leadin}}</p>
        </copy-wrap>
    </stellar-layout>
    <stellar-layout padding="none" size="full">
        <stellar-grid>
            {% for track in es.tracks %}
                {% include track %}
            {% endfor %}
        </stellar-grid>
    </stellar-layout>
</stellar-layout>