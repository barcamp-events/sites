    <stellar-layout padding="none" size="flush" style="--grid-gap: 0;">
        <stellar-tabs block class="bg-transparent bn" flip-indicator invert pay-attention>
            <stellar-tab class="ttu fw7 tracked" href="#about" tag="link">{{en.event.about.title | downcase | remove: "the"}}</stellar-tab>
            <stellar-tab class="ttu fw7 tracked" href="#venue" tag="link">{{en.event.venue.title | downcase | remove: "the"}}</stellar-tab>
            <stellar-tab class="ttu fw7 tracked" href="#tracks" tag="link">{{en.event.tracks.title | downcase | remove: "the"}}</stellar-tab>
            <stellar-tab class="ttu fw7 tracked" href="#agenda" tag="link">{{en.event.agenda.title | downcase | remove: "the"}}</stellar-tab>
            <stellar-tab class="ttu fw7 tracked" href="#faqs" tag="link">{{en.event.faqs.title | downcase | remove: "the"}}</stellar-tab>
            <stellar-tab class="ttu fw7 tracked" href="#sponsors" tag="link">{{en.event.sponsors.title | downcase | remove: "the"}}</stellar-tab>
        </stellar-tabs>
    </stellar-layout>
